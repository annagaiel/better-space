<div class="col-md-8 col-md-offset-2 landing">
  <h2>Where do you need space? </h2>
  <%= form_tag "/space_listings", method: "get", class:"form-group" do %>
    <div class="input-group">
      <%= text_field_tag :search_term, nil, id:"searchTextField", class:"form-control", placeholder:"Enter your Zipcode", required:true %>
      <span class="input-group-btn">
        <%= submit_tag "Search", class:"btn-sm btn-danger landing-search" %>
      </span>
    </div>
  <% end %>
</div>
